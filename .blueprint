models:
    Faq: { question: string, answer: text, faq_category_id: 'biginteger unsigned', order: integer, is_active: boolean }
    FaqCategory: { name: string, order: integer, is_active: boolean }
    SupportTicket: { title: string, full_name: string, email: string, subject: "enum:'General','Technical','Accounts','Payment','Other'", description: text, status: "enum:'open','in-progress','resolved','closed' default:open" }
    User: { username: 'string nullable', email: string, email_verified_at: 'timestamp nullable', password: 'string nullable', provider_id: 'string nullable', provider_name: 'string nullable', provider_token: 'string nullable', provider_refresh_token: 'string nullable', remember_token: 'string:100 nullable', profile_picture: 'string nullable', id_number: 'string nullable', phone_number: 'string nullable', first_name: 'string nullable', surname: 'string nullable', date_of_birth: 'date nullable', place_of_birth: 'string nullable', education: 'string nullable', institution: 'string nullable', address: 'text nullable' }
    CourseCategory: { name: string, relationships: { hasMany: Course } }
    Course: { title: string, teacher_id: 'id foreign:users', course_category_id: 'id foreign:course_categories', description: longtext, thumbnail: string, level: 'enum:beginner,intermediate,advanced', duration: integer, extra_description: longtext, is_published: boolean, created_by: string, modified_by: string, timestamps: timestamps }
    CourseSyllabus: { course_id: 'id foreign:courses', title: string, order: integer, is_completed: boolean, created_by: string, modified_by: string, timestamps: timestamps }
    CourseContent: { course_syllabus_id: 'id foreign:course_syllabus', title: string, content: longtext, video_url: string, order: integer, is_free_preview: boolean, is_assessment: boolean, is_completed: boolean, created_by: string, modified_by: string, timestamps: timestamps }
    Quiz: { course_syllabus_id: 'id foreign:course_syllabus', title: string, number_of_questions: integer, duration: integer, created_by: string, modified_by: string, timestamps: timestamps }
    QuizQuestion: { quiz_id: 'id foreign:quizzes', question: longtext, question_type: 'enum:multiple_choice,short_answer,true_false', score: integer, order: integer, created_by: string, modified_by: string, timestamps: timestamps }
    QuizChoice: { question_id: 'id foreign:quiz_questions', answer_option: string, is_correct: boolean, created_by: string, modified_by: string, timestamps: timestamps }
    QuizAttempt: { quiz_id: 'id foreign:quizzes', user_id: 'id foreign:users', start_time: timestamp, end_time: timestamp, total_score: integer, status: 'enum:in_progress,submitted,graded', timestamps: timestamps }
    QuizAnswer: { quiz_attempt_id: 'id foreign:quiz_attempts', quiz_question_id: 'id foreign:quiz_questions', answer: longtext, is_correct: boolean, score: integer, timestamps: timestamps }
